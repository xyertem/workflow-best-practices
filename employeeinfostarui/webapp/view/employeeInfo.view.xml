<mvc:View controllerName="demo.workflow.dev.employeeinfostarui.controller.employeeInfo"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="_IDGenPage1" title="Workflow Start UI">
    <content>
        <VBox width="100%" direction="Column" id="__vbox0">
            <items>
                <Input width="100%" id="textInput" value="{/text}"/>
                <Button text="Start Workflow" width="100px" id="__button0" press="startWorkflow"/>
                <Text text="{/result}" maxLines="0" id="__text0"/>
            </items>
        </VBox>
    </content>
</Page>
</mvc:View>
